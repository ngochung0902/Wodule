<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/color_background">
    <LinearLayout
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_above="@+id/ll_group_bottom"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:background="@drawable/boder_bg_content">
        <TextView
            android:text="ASSESSMENT OVERVIEW"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:textColor="@color/color_white"
            android:textSize="@dimen/text14"
            android:drawableLeft="@mipmap/ic_newuser"
            android:padding="@dimen/margin10"
            android:gravity="center_vertical"
            android:drawablePadding="@dimen/margin5"
            android:layout_marginLeft="@dimen/margin5"
            android:layout_gravity="center_horizontal"
            android:id="@+id/lbTitle" />

        <LinearLayout
            android:id="@+id/ll_group_center"
            android:orientation="vertical"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:background="@drawable/border_rect_greentopbottom">
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_weight="1">
                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent">
                    <TextView
                        android:textSize="@dimen/text14"
                        android:layout_margin="@dimen/margin10"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:gravity="left"
                        android:text="PART 1: Reading Aloud"
                        android:textColor="@color/color_white"
                        android:id="@+id/textView1" />
                    <TextView
                        android:drawableLeft="@mipmap/ic_textview"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:gravity="center"
                        android:text="VIEW TEXT"
                        android:textSize="10dp"
                        android:textColor="@color/color_white"
                        android:layout_marginLeft="@dimen/margin10"
                        android:layout_marginRight="@dimen/margin10"
                        android:layout_alignTop="@+id/textView1"
                        android:layout_alignParentEnd="true"
                        android:id="@+id/textView2" />

                    <LinearLayout
                        android:orientation="horizontal"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_below="@+id/textView2"
                        android:layout_alignParentStart="true">
                        <LinearLayout
                            android:layout_marginTop="@dimen/margin10"
                            android:layout_marginBottom="@dimen/margin10"
                            android:layout_marginLeft="@dimen/margin10"
                            android:background="#3d9463"
                            android:layout_weight="1"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent">
                        </LinearLayout>
                        <LinearLayout
                            android:layout_marginTop="@dimen/margin10"
                            android:layout_marginBottom="@dimen/margin10"
                            android:layout_marginRight="@dimen/margin10"
                            android:background="#75c296"
                            android:layout_weight="1"
                            android:gravity="center"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent">
                            <TextView
                                android:gravity="center"
                                android:text="SCORE "
                                android:drawableRight="@mipmap/ic_dropdown"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:textSize="@dimen/text14"
                                android:textColor="@color/color_white"
                                android:layout_gravity="center"
                                android:background="@drawable/border_rect_green_dark"/>
                        </LinearLayout>
                    </LinearLayout>
                </RelativeLayout>
            </LinearLayout>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_weight="1">
                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent">
                    <TextView
                        android:textSize="@dimen/text14"
                        android:layout_margin="@dimen/margin10"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:gravity="left"
                        android:text="PART 2: Talk about this Picture"
                        android:textColor="@color/color_white"
                        android:id="@+id/textView4" />
                    <TextView
                        android:drawableLeft="@mipmap/ic_textview"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:gravity="center"
                        android:text="VIEW PHOTO"
                        android:textSize="10dp"
                        android:textColor="@color/color_white"
                        android:layout_marginLeft="@dimen/margin10"
                        android:layout_marginRight="@dimen/margin10"
                        android:layout_alignTop="@+id/textView4"
                        android:layout_alignParentEnd="true"
                        android:id="@+id/textView5" />

                    <LinearLayout
                        android:orientation="horizontal"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_below="@+id/textView4"
                        android:layout_alignParentStart="true">
                        <LinearLayout
                            android:layout_marginTop="@dimen/margin10"
                            android:layout_marginBottom="@dimen/margin10"
                            android:layout_marginLeft="@dimen/margin10"
                            android:background="#3d9463"
                            android:layout_weight="1"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent">
                        </LinearLayout>
                        <LinearLayout
                            android:layout_marginTop="@dimen/margin10"
                            android:layout_marginBottom="@dimen/margin10"
                            android:layout_marginRight="@dimen/margin10"
                            android:background="#75c296"
                            android:layout_weight="1"
                            android:gravity="center"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent">
                            <TextView
                                android:gravity="center"
                                android:text="SCORE "
                                android:drawableRight="@mipmap/ic_dropdown"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:textSize="@dimen/text14"
                                android:textColor="@color/color_white"
                                android:layout_gravity="center"
                                android:background="@drawable/border_rect_green_dark"/>
                        </LinearLayout>
                    </LinearLayout>
                </RelativeLayout>
            </LinearLayout>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_weight="1">
                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent">
                    <TextView
                        android:textSize="@dimen/text14"
                        android:layout_margin="@dimen/margin10"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:gravity="left"
                        android:text="PART 3: Spoken Interaction | What is your hobby?"
                        android:textColor="@color/color_white"
                        android:id="@+id/textView6" />
                    <TextView
                        android:visibility="invisible"
                        android:drawableLeft="@mipmap/ic_textview"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:gravity="center"
                        android:text="VIEW TEXT"
                        android:textSize="10dp"
                        android:textColor="@color/color_white"
                        android:layout_marginLeft="@dimen/margin10"
                        android:layout_marginRight="@dimen/margin10"
                        android:layout_alignTop="@+id/textView6"
                        android:layout_alignParentEnd="true"
                        android:id="@+id/textView7" />

                    <LinearLayout
                        android:orientation="horizontal"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_below="@+id/textView7"
                        android:layout_alignParentStart="true">
                        <LinearLayout
                            android:layout_marginTop="@dimen/margin10"
                            android:layout_marginBottom="@dimen/margin10"
                            android:layout_marginLeft="@dimen/margin10"
                            android:background="#3d9463"
                            android:layout_weight="1"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent">
                        </LinearLayout>
                        <LinearLayout
                            android:layout_marginTop="@dimen/margin10"
                            android:layout_marginBottom="@dimen/margin10"
                            android:layout_marginRight="@dimen/margin10"
                            android:background="#75c296"
                            android:layout_weight="1"
                            android:gravity="center"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent">
                            <TextView
                                android:gravity="center"
                                android:text="SCORE "
                                android:drawableRight="@mipmap/ic_dropdown"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:textSize="@dimen/text14"
                                android:textColor="@color/color_white"
                                android:layout_gravity="center"
                                android:background="@drawable/border_rect_green_dark"/>
                        </LinearLayout>
                    </LinearLayout>
                </RelativeLayout>
            </LinearLayout>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_weight="1">
                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent">
                    <TextView
                        android:textSize="@dimen/text14"
                        android:layout_margin="@dimen/margin10"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:gravity="left"
                        android:text="PART 4: Spoken Interaction | What is your favorite book?"
                        android:textColor="@color/color_white"
                        android:id="@+id/textView" />
                    <TextView
                        android:visibility="invisible"
                        android:drawableLeft="@mipmap/ic_textview"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:gravity="center"
                        android:text="VIEW TEXT"
                        android:textSize="10dp"
                        android:textColor="@color/color_white"
                        android:layout_marginLeft="@dimen/margin10"
                        android:layout_marginRight="@dimen/margin10"
                        android:layout_alignTop="@+id/textView"
                        android:layout_alignParentEnd="true"
                        android:id="@+id/textView8" />

                    <LinearLayout
                        android:orientation="horizontal"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_below="@+id/textView8"
                        android:layout_alignParentStart="true">
                        <LinearLayout
                            android:layout_marginTop="@dimen/margin10"
                            android:layout_marginBottom="@dimen/margin10"
                            android:layout_marginLeft="@dimen/margin10"
                            android:background="#3d9463"
                            android:layout_weight="1"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent">
                        </LinearLayout>
                        <LinearLayout
                            android:layout_marginTop="@dimen/margin10"
                            android:layout_marginBottom="@dimen/margin10"
                            android:layout_marginRight="@dimen/margin10"
                            android:background="#75c296"
                            android:layout_weight="1"
                            android:gravity="center"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent">
                            <TextView
                                android:gravity="center"
                                android:text="SCORE "
                                android:drawableRight="@mipmap/ic_dropdown"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:textSize="@dimen/text14"
                                android:textColor="@color/color_white"
                                android:layout_gravity="center"
                                android:background="@drawable/border_rect_green_dark"/>
                        </LinearLayout>
                    </LinearLayout>
                </RelativeLayout>
            </LinearLayout>
        </LinearLayout>
    </LinearLayout>


    <LinearLayout
        android:background="@color/color_background"
        android:id="@+id/ll_group_bottom"
        android:orientation="horizontal"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true">
        <LinearLayout
            android:orientation="vertical"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:gravity="left">
            <TextView
                android:layout_marginTop="@dimen/margin10"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Examinee ID:"
                android:textColor="@color/color_white"
                android:layout_marginLeft="@dimen/margin20" />
            <TextView
                android:layout_marginTop="@dimen/margin5"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="1536-AF-4581"
                android:textColor="@color/color_white"
                android:layout_marginLeft="@dimen/margin20" />
        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:gravity="right">
            <TextView
                android:text="Submit"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/btnSubmit"
                android:textSize="@dimen/text16"
                android:textColor="@color/color_white"
                android:layout_gravity="center"
                android:paddingLeft="@dimen/margin10"
                android:paddingRight="@dimen/margin10"
                android:layout_marginTop="@dimen/margin15"
                android:layout_marginBottom="@dimen/margin15"
                android:layout_marginRight="@dimen/margin30"
                android:background="@drawable/border_rect_green_dark"/>
        </LinearLayout>


    </LinearLayout>

</RelativeLayout>

